{{#each adminuser}}
<tr>
                        <td>{{ firstName }} {{lastName}}</td>
                        <td>{{ username }}</td>
                        <td>{{ mobileNo }}</td>
                        <td>{{ email }}</td>
                        <td>latitude: {{ latitude }} <br> longitude: {{longitude}}</td>
                        <td>
                           <form style="display: inline;" action="/deletegarnus/{{ this._id }}" method="POST" onsubmit="return confirm('Are you sure you want to delete this item?');">
                              <button style="cursor: pointer;" type="submit" class="btn btn-danger ml-2">Delete</button>
                           </form>
                        </td>
   </tr>
{{/each}}

